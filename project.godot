config_version=5

[application]

config/name="PistonPrecision"
config/description="Control a piston player, resolve puzzles and go fast to bait you're previous score "
run/main_scene="res://scenes/worlds/MainScene.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://scripts/managers/GameManager.gd"
InputManager="*res://scripts/managers/InputManager.gd"
AudioManager="*res://scripts/managers/AudioManager.gd"
ParticleManager="*res://scripts/managers/ParticleManager.gd"
DebugManager="*res://scripts/managers/DebugManager.gd"
SceneManager="*res://scripts/managers/SceneManager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"

[file_customization]

folder_colors={
"res://assets/": "red",
"res://audio/": "purple",
"res://data/": "pink",
"res://resources/": "yellow",
"res://scenes/": "green",
"res://scripts/": "teal"
}

